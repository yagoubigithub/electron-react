{
  "name": "electron-react",
  "version": "1.0.0",
  "description": "",
  "main": "app/electron/main.js",
  "scripts": {
    "dev-server": "webpack serve --mode development --config ./webpack.development.js",
    "dev": "cross-env NODE_ENV=development electron .",    
    "preprod": "webpack --mode production --config ./webpack.production.js",
    "prod": "cross-env NODE_ENV=production electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "electronreact",
    "appId": "com.electron.react.webpack",
    "directories": {
        "buildResources": "resources"
    },
    "win": {
        "target": [
            "nsis",
            "msi"
        ]
    },
    "linux": {
        "target": [
            "deb",
            "rpm",
            "snap",
            "AppImage"
        ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yagoubigithub/electron-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yagoubigithub/electron-react/issues"
  },
  "homepage": "https://github.com/yagoubigithub/electron-react#readme",
  "devDependencies": {
    "babel-loader": "9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^26.2.3",
    "electron-builder": "^24.6.4",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "postcss-loader": "^7.3.3",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
